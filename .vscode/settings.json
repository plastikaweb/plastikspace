{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/.js": {
      "when": "$(basename).ts"
    },
    "**/.js.map": {
      "when": "$(basename)"
    }
  },
  "projectManager.git.ignoredFolders": ["out", "typings", "test", ".haxelib"],
  "projectManager.hg.ignoredFolders": ["out", "typings", "test", ".haxelib"],
  "projectManager.svn.ignoredFolders": ["out", "typings", "test"],
  "projectManager.any.ignoredFolders": ["out", "typings", "test"],
  "projectManager.vscode.ignoredFolders": ["out", "typings", "test"],
  "svg.previewAll.exclude": "",
  "markdown.extension.completion.respectVscodeSearchExclude": false
}
