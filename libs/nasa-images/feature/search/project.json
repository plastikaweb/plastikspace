{
  "name": "nasa-images-feature-search",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/nasa-images/feature/search/src",
  "prefix": "plastik",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/nasa-images/feature/search/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/nasa-images/feature/search/**/*.ts", "libs/nasa-images/feature/search/**/*.html"]
      }
    }
  },
  "tags": ["scope:nasa-images", "type:feature"]
}
