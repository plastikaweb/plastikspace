{
  "name": "nasa-images-faqs-feature",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/nasa-images/faqs/feature/src",
  "prefix": "plastik",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/nasa-images/faqs/feature/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/nasa-images/faqs/feature/**/*.ts", "libs/nasa-images/faqs/feature/**/*.html"]
      }
    }
  },
  "tags": ["scope:nasa-images", "type:feature"]
}
