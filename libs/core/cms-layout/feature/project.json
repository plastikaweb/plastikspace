        "jestConfig": "libs/core/cms-layout/feature/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/core/cms-layout/feature/**/*.ts", "libs/core/cms-layout/feature/**/*.html"]
      }
    }
  },
  "tags": ["scope:core", "type:feature"]
}
