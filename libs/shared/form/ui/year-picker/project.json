{
  "name": "shared-form-ui-year-picker",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/form/ui/year-picker/src",
  "prefix": "plastik",
  "tags": ["type:ui", "scope:shared"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/form/ui/year-picker/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/form/ui/year-picker/**/*.ts", "libs/shared/form/ui/year-picker/**/*.html"]
      }
    }
  }
}
