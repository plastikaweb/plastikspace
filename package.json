{
  "name": "plastikspace",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "build": "nx run-many --target=build --all --parallel",
    "test": "nx test",
    "test:watch": "nx test --watch",
    "test:coverage": "nx test --coverage",
    "test:all": "nx run-many --all --target=test --ci --passWithNoTests --parallel --max-parallel=3 --run-in-band --coverage --max-warnings=0",
    "test:ci": "nx affected --target=test --base=remotes/origin/develop --parallel",
    "lint": "nx lint",
    "lint:all": "nx run-many --all --target=lint --parallel --max-parallel=3 --max-warnings=0",
    "lint:ci": "nx affected --target=lint --base=remotes/origin/develop --head=HEAD --parallel",
    "e2e": "ng e2e",
    "e2e:all": "nx run-many --all --target=e2e --headed=false --watch=false --parallel",
    "e2e:ci": "nx affected --target=e2e --base=remotes/origin/develop --parallel --headed=false --watch=false",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build --configuration=production",
    "affected:e2e": "nx affected:e2e",
    "affected:e2e:watch": "nx affected:e2e --watch",
    "affected:test": "nx affected:test --parallel --max-parallel=3 --run-in-band --max-warnings=0",
    "affected:lint": "nx affected:lint --parallel --max-parallel=3 --verbose --cache --max-warnings=0",
    "affected:dep-graph": "nx affected:dep-graph",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "prepare": "husky install",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "husky-install": "husky install",
    "clean": "rimraf node_modules",
    "clean:all": "rimraf node_modules dist dll && yarn cache clean --force",
    "clean:dist": "rimraf dist",
    "install:local": "yarn husky-install && yarn clean:all && yarn install --frozen-lockfile",
    "markdownlint": "markdownlint *.md",
    "nasa-images:serve": "nx run nasa-images:serve",
    "nasa-images:lint": "nx run nasa-images:lint",
    "nasa-images:build": "nx run nasa-images:build --parallel",
    "nasa-images:test": "nx run nasa-images:test",
    "nasa-images:e2e": "nx run nasa-images-e2e:e2e",
    "nasa-images:build:github": "nx run nasa-images:build --deploy-url=https://plastikaweb.github.io/nasa-images/ --base-href=/nasa-images",
    "branch:lint": "branch-name-lint branchNameLint.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.2.1",
    "@angular/cdk": "15.2.1",
    "@angular/common": "15.2.1",
    "@angular/compiler": "15.2.1",
    "@angular/core": "15.2.1",
    "@angular/forms": "15.2.1",
    "@angular/material": "15.2.1",
    "@angular/platform-browser": "15.2.1",
    "@angular/platform-browser-dynamic": "15.2.1",
    "@angular/router": "15.2.1",
    "@ngrx/component": "15.3.0",
    "@ngrx/component-store": "15.3.0",
    "@ngrx/effects": "15.3.0",
    "@ngrx/entity": "15.3.0",
    "@ngrx/router-store": "15.3.0",
    "@ngrx/store": "15.3.0",
    "@ngx-formly/core": "^6.1.3",
    "@ngx-formly/material": "^6.1.3",
    "@nrwl/angular": "15.8.5",
    "@nrwl/nx-cloud": "^15.3.4",
    "angular-svg-icon": "^15.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.1",
    "@angular-devkit/core": "15.2.1",
    "@angular-devkit/schematics": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.0",
    "@angular/compiler-cli": "15.2.1",
    "@angular/language-service": "15.2.1",
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@ngrx/schematics": "15.3.0",
    "@ngrx/store-devtools": "15.3.0",
    "@ngx-formly/schematics": "^6.1.3",
    "@nrwl/cli": "15.8.5",
    "@nrwl/cypress": "15.8.5",
    "@nrwl/eslint-plugin-nx": "15.8.5",
    "@nrwl/jest": "15.8.5",
    "@nrwl/js": "15.8.5",
    "@nrwl/linter": "15.8.5",
    "@nrwl/workspace": "15.8.5",
    "@schematics/angular": "15.2.1",
    "@types/jest": "29.4.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "autoprefixer": "^10.4.13",
    "branch-name-lint": "^2.1.1",
    "commitizen": "^4.2.6",
    "cypress": "^12.2.0",
    "cz-customizable": "^7.0.0",
    "cz-customizable-ghooks": "^2.0.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-deprecation": "^1.3.3",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-ngrx": "^2.1.4",
    "husky": "^8.0.2",
    "jasmine-marbles": "~0.9.1",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "jest-preset-angular": "13.0.0",
    "markdownlint": "^0.28.0",
    "markdownlint-cli": "^0.33.0",
    "nx": "15.8.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "rimraf": "^4.3.1",
    "tailwindcss": "^3.2.4",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  }
}
